{
  "manifest_version": 2,
  "name": "Historium",
  "version": "0.1",

  "description": "Extended history functionality",

  "icons": {
    "48": "icon-48.png",
    "96": "icon-96.png"
  },

  "permissions" : [
    "history"
  ],

  "browser_action": {
    "default_icon": "icon-32.png",
    "default_title": "Historium"
  },

  "background": {
    "scripts": ["background.js"]
  }
}